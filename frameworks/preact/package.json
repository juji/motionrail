{
  "name": "@motionrail/preact",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite build --watch\" \"tsc --emitDeclarationOnly --watch\"",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "main": "./dist/preact.umd.js",
  "module": "./dist/preact.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/preact.es.js",
      "require": "./dist/preact.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "motionrail": "workspace:*"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "prettier": "^3.8.0"
  }
}
