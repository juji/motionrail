{
  "name": "@motionrail/qwik",
  "version": "0.0.1",
  "description": "MotionRail wrapper for Qwik",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "main": "./dist/qwik.umd.js",
  "module": "./dist/qwik.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/qwik.es.js",
      "require": "./dist/qwik.umd.js"
    }
  },
  "files": ["dist"],
  "dependencies": {
    "motionrail": "workspace:*"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^1.0.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@eslint/js": "latest",
    "@types/node": "20.19.0",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.18.0",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vite": "7.2.6",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
