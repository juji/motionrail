{
  "name": "vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/vue.umd.js",
  "module": "./dist/vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue.es.js",
      "require": "./dist/vue.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"vite build --watch\" \"vue-tsc --project tsconfig.app.json --watch\"",
    "build": "vite build && vue-tsc --project tsconfig.app.json",
    "preview": "vite preview",
    "check": "vue-tsc --project tsconfig.app.json --noEmit",
    "format": "prettier --write ."
  },
  "dependencies": {
    "motionrail": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "vue": "^3.5.24",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vue-tsc": "^3.1.4",
    "prettier": "^3.8.0"
  }
}
