{
  "name": "motionrail",
  "version": "0.5.0",
  "description": "",
  "main": "./main/dist/motionrail.umd.js",
  "module": "./main/dist/motionrail.es.js",
  "types": "./main/dist/motionrail.d.ts",
  "exports": {
    ".": {
      "types": "./main/dist/motionrail.d.ts",
      "import": "./main/dist/motionrail.es.js",
      "require": "./main/dist/motionrail.umd.js"
    },
    "./style.css": "./main/dist/style.css",
    "./extensions/logger": {
      "types": "./extensions/logger/dist/logger.d.ts",
      "import": "./extensions/logger/dist/logger.es.js",
      "require": "./extensions/logger/dist/logger.umd.js"
    },
    "./extensions/arrows/style.css": "./extensions/arrows/dist/style.css",
    "./extensions/arrows": {
      "types": "./extensions/arrows/dist/arrows.d.ts",
      "import": "./extensions/arrows/dist/arrows.es.js",
      "require": "./extensions/arrows/dist/arrows.umd.js"
    },
    "./extensions/dots/style.css": "./extensions/dots/dist/style.css",
    "./extensions/dots": {
      "types": "./extensions/dots/dist/dots.d.ts",
      "import": "./extensions/dots/dist/dots.es.js",
      "require": "./extensions/dots/dist/dots.umd.js"
    },
    "./extensions/thumbnails/style.css": "./extensions/thumbnails/dist/style.css",
    "./extensions/thumbnails": {
      "types": "./extensions/thumbnails/dist/thumbnails.d.ts",
      "import": "./extensions/thumbnails/dist/thumbnails.es.js",
      "require": "./extensions/thumbnails/dist/thumbnails.umd.js"
    },
    "./react": {
      "types": "./frameworks/react/dist/main.d.ts",
      "import": "./frameworks/react/dist/react.es.js",
      "require": "./frameworks/react/dist/react.umd.js"
    },
    "./vue": {
      "types": "./frameworks/vue/dist/index.d.ts",
      "import": "./frameworks/vue/dist/vue.es.js",
      "require": "./frameworks/vue/dist/vue.umd.js"
    },
    "./svelte": {
      "types": "./frameworks/svelte/dist/index.d.ts",
      "svelte": "./frameworks/svelte/dist/index.js",
      "default": "./frameworks/svelte/dist/index.js"
    },
    "./preact": {
      "types": "./frameworks/preact/dist/main.d.ts",
      "import": "./frameworks/preact/dist/preact.es.js",
      "require": "./frameworks/preact/dist/preact.umd.js"
    },
    "./solid": {
      "types": "./frameworks/solid/dist/main.d.ts",
      "import": "./frameworks/solid/dist/solid.es.js",
      "require": "./frameworks/solid/dist/solid.umd.js"
    },
    "./lit": {
      "types": "./frameworks/lit/dist/main.d.ts",
      "import": "./frameworks/lit/dist/lit.es.js",
      "require": "./frameworks/lit/dist/lit.umd.js"
    },
    "./qwik": {
      "types": "./frameworks/qwik/dist/main.d.ts",
      "import": "./frameworks/qwik/src/lib/main.tsx"
    }
  },
  "files": [
    "main/dist",
    "extensions/logger/dist",
    "extensions/arrows/dist",
    "extensions/dots/dist",
    "extensions/thumbnails/dist",
    "frameworks/react/dist",
    "frameworks/vue/dist",
    "frameworks/svelte/dist",
    "frameworks/preact/dist",
    "frameworks/solid/dist",
    "frameworks/lit/dist",
    "frameworks/qwik/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm build:main && pnpm build:libs && pnpm build:rest",
    "build:main": "pnpm --filter main build",
    "build:libs": "pnpm --filter './extensions/*' --filter './frameworks/*' build",
    "build:rest": "pnpm -r --filter '!main' --filter '!./extensions/*' --filter '!./frameworks/*' build",
    "dev": "pnpm --filter './manual/*' dev"
  },
  "keywords": [
    "carousel",
    "slider",
    "slideshow",
    "swipe",
    "touch",
    "drag",
    "momentum",
    "snap",
    "responsive",
    "breakpoints",
    "autoplay",
    "rtl",
    "react",
    "vue",
    "svelte",
    "solid",
    "typescript",
    "lightweight",
    "zero-dependencies"
  ],
  "author": "jujiyangasli@gmail.com",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0"
}
