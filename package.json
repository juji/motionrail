{
  "name": "motionrail",
  "version": "0.0.3",
  "type": "module",
  "module": "./dist/motionrail.js",
  "types": "./dist/motionrail.d.ts",
  "exports": {
    ".": {
      "import": "./dist/motionrail.js",
      "types": "./dist/motionrail.d.ts"
    },
    "./extensions/arrows": {
      "import": "./dist/extensions/arrows.js",
      "types": "./dist/extensions/arrows/main.d.ts"
    },
    "./extensions/arrows/style.css": "./dist/extensions/arrows/style.css",
    "./extensions/logger": {
      "import": "./dist/extensions/logger.js",
      "types": "./dist/extensions/logger.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/juji/motionrail"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js,json,css,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "husky": "^9.1.7"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "dependencies": {}
}
