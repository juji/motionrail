#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/juji/play/motionrail/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules/rolldown-vite/bin/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules/rolldown-vite/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/juji/play/motionrail/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules/rolldown-vite/bin/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules/rolldown-vite/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules/rolldown-vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@25.0.9_less@4.5.1_sass@1.97.2_stylus@0.62.0/node_modules/rolldown-vite/bin/vite.js" "$@"
fi
