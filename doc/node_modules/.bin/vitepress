#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/juji/play/motionrail/node_modules/.pnpm/vitepress@2.0.0-alpha.15_@algolia+client-search@5.46.3_@types+node@25.0.9_@types+react@_62df14302d06f737e2ede5052fea3d49/node_modules/vitepress/bin/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/vitepress@2.0.0-alpha.15_@algolia+client-search@5.46.3_@types+node@25.0.9_@types+react@_62df14302d06f737e2ede5052fea3d49/node_modules/vitepress/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/vitepress@2.0.0-alpha.15_@algolia+client-search@5.46.3_@types+node@25.0.9_@types+react@_62df14302d06f737e2ede5052fea3d49/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/juji/play/motionrail/node_modules/.pnpm/vitepress@2.0.0-alpha.15_@algolia+client-search@5.46.3_@types+node@25.0.9_@types+react@_62df14302d06f737e2ede5052fea3d49/node_modules/vitepress/bin/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/vitepress@2.0.0-alpha.15_@algolia+client-search@5.46.3_@types+node@25.0.9_@types+react@_62df14302d06f737e2ede5052fea3d49/node_modules/vitepress/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/vitepress@2.0.0-alpha.15_@algolia+client-search@5.46.3_@types+node@25.0.9_@types+react@_62df14302d06f737e2ede5052fea3d49/node_modules:/Users/juji/play/motionrail/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
