{
  "name": "main",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/motionrail.umd.js",
  "module": "./dist/motionrail.es.js",
  "types": "./dist/motionrail.d.ts",
  "exports": {
    ".": {
      "types": "./dist/motionrail.d.ts",
      "import": "./dist/motionrail.es.js",
      "require": "./dist/motionrail.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"vite build --watch\" \"tsc --watch\"",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "concurrently": "^9.1.2",
    "typescript": "~5.9.3",
    "prettier": "^3.8.0",
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
