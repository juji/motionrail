{
  "name": "@motionrail/dots",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/dots.umd.js",
  "module": "./dist/dots.es.js",
  "types": "./dist/dots.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dots.d.ts",
      "import": "./dist/dots.es.js",
      "require": "./dist/dots.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"vite build --watch\" \"tsc --watch\"",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "concurrently": "^9.2.1",
    "motionrail": "workspace:*",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
